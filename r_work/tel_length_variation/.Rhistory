library(tidyverse)
briggsae_length <- read.delim("~/thesis/worm/result_data/Length_estimates/briggsae_length.tsv", quote="")
View(briggsae_length)
brigg_isotype_groups <- read.delim("~/thesis/worm/data/brigg/brigg_isotype_groups.tsv", quote="")
View(brigg_isotype_groups)
brigg_isolist <- as_tibble(brigg_isotype_groups)
brigg_len <- as_tibble(briggsae_length)
view(brigg_len)
merge(brigg_len,brigg_isolist)
brigg_isolist <- brigg_isolist %>% select(group,strain)
merge(brigg_isolist,brigg_len)
rename %>% brigg_len(strain=brigg_strain)
brigg_len %>% rename(strain=brigg_strain)
brigg_len <- brigg_len %>% rename(strain=brigg_strain)
merge(brigg_len,brigg_isolist)
strain_isotype_brigg <- merge(brigg_len,brigg_isolist)
View(strain_isotype_brigg)
write_tsv(strain_isotype_brigg,briggsae_strain_iso_length)
write_tsv(strain_isotype_brigg,"briggsae_strain_iso_length")
